pollutantmean <- function(directory, pollutant,id=1:332) {
a <- getwd()
mydir <- file.path(a,directory)
b <- dir()
b <- b[id]
for (i in b) {
myfile <- file.path(mdir,i)
f <- read.csv(file=myfile)
if (pollutant == 'nitrate') {
mymean <- mean(f$nitrate, na.rm=TRUE)
} else {
mymean <- mean(f$sulfate, na.rm=TRUE)
}
}
return(mymean)
}

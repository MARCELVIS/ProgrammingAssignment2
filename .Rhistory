corrsummary <- function(directory,threshold =0) {
cornum <- numeric(0)
dfnobs <- complete("specdata")
dfnobs <- dfnobs[dfnobs$nobs > threshold, ]
for (cid in dfnobs$id) {
dfrid <- getmonitor(cid, directory)
corNum <- c(corNum, cor(dfrid$sulfate, dfrid$nitrate, use = "pairwise.complete.obs"))
}
return(cornum)
}
